{
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
  "workbench.colorCustomizations": {
    // "titleBar.activeBackground": "#1B2229",
    // "titleBar.inactiveBackground": "#1B2229",
    // "titleBar.activeBackground": "#191919",
    // "titleBar.inactiveBackground": "#191919",
  },
  "window.titleBarStyle": "native",
  "vscode_custom_css.imports": [
    "file:///Users/kyloren/Library/Application Support/Code/User/vscode.css"
  ],
  "terminal.external.osxExec": "iTerm.app",
  "workbench.editor.showIcons": false,
  //VIM CONFIG

  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.replace": "underline-thin",
  "vim.easymotionMarkerBackgroundColor": "#EEB32D",
  "vim.visualModeKeyBindings": [
    {
      "before": ["p"],
      "after": ["p", "gv", "y", "<Esc>"]
    }
  ],
  "vim.easymotion": true,
  "vim.surround": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<Tab>"],
      "after": ["i", "<C-t>", "<Esc>"]
    },
    {
      "before": ["<leader>", "<S-Tab>"],
      "after": ["i", "<C-d>", "<Esc>"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<Esc>"]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "editor.lineNumbers": "relative",
  "editor.fontSize": 13,
  "editor.letterSpacing": 1,
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrains Mono, monospace",
  "files.autoSave": "onWindowChange",
  "workbench.editor.untitled.hint": "hidden",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,

  "customizeUI.stylesheet": {
    // "body.inline-title-bar .monaco-workbench:not(.fullscreen) .sidebar .composite.title": "background: rgba(0, 0, 0, 0) !important;",
